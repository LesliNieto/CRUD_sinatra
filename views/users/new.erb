<h1> Registration Form </h1>
<form id="user_form" action="/users/new" method="post">
  <label>Name: </label>
  <input required type="text" name="name" placeholder="Name" id="name" autocomplete="off">
  <br><br><label>Last Name: </label>
  <input required type="text" name="last_name" placeholder="Last Name" id="last_name" autocomplete="off">
  <br><br><label>Email: </label>
  <input required type="email" name="email" placeholder="example@gmail.com" id="email" autocomplete="off">
  <br><br><label>Age: </label>
  <input  type="number" name="age" placeholder="Age" id="age" autocomplete="off">
  <br><br><label>Address: </label>
  <input  type="text" name="address" placeholder="Address" id="address" autocomplete="off">
  <br><br><center><input class="button" type="submit" name="save" value="Save"></center>
</form>
